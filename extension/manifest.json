{
  "name": "Codechat",
  "version": "2.2.8",
  "manifest_version": 9,
  "description": "Chat over any web resource with other people in the same resource",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "icons":{
    "128": "icon.png"
  },
  "background": {
    "scripts": [
      "socket.io.js",
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "https://*/*",
    "http://*/*",
    "<all_urls>"
  ]
}